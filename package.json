{
  "name": "@comake/solid-redis",
  "version": "1.0.0",
  "description": "A redis database backend for a Community Solid Server",
  "keywords": [
    "solid",
    "linked-data",
    "rdf",
    "ldp",
    "redis"
  ],
  "license": "MIT",
  "author": "Adler Faulkner <adler@comake.io>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:comake/solid-redis.git",
  "bugs": {
    "url": "https://github.com/comake/solid-redis/issues"
  },
  "homepage": "https://github.com/comake/solid-redis#readme",
  "files": [
    "config",
    "dist"
  ],
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@comake/solid-redis",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/solid-redis/^1.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/solid-redis/^1.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/solid-redis/^1.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@comake/solid-redis/^1.0.0/dist/": "dist/"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore",
    "build:ts": "tsc",
    "jest": "jest --coverage",
    "lint": "eslint . --cache",
    "test": "npm run test:ts && npm run jest",
    "test:ts": "tsc -p test --noEmit",
    "prepare": "npm run build"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "arrayify-stream": "^2.0.0",
    "n3": "^1.16.0",
    "rdf-string": "^1.6.0",
    "redis": "^4.0.6"
  },
  "peerDependencies": {
    "@solid/community-server": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@solid/community-server": "^3.0.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "componentsjs-generator": "^2.0.0",
    "eslint": "^7.16.0",
    "eslint-config-es": "^3.26.13",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "eslint-plugin-unused-imports": "^1.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-rdf": "^1.7.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.1.3"
  }
}
